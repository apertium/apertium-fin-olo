  <section-def-macros>
    <def-macro n="remove-semtag1" npar="1">
          <let>
            <clip pos="1" side="tl" part="a_sem"/>
            <lit v=""/>
          </let>
        </def-macro>
    <def-macro n="tensemood-mangler" npar="1">
      <choose>
        <when>
          <test>
              <equal><clip pos="1" side="sl" part="a_tense"/><lit-tag v="past"/></equal>
          </test>
          <let>
            <clip pos="1" side="tl" part="a_tense"/><lit-tag v="ind.prt"/>
          </let>
        </when>
        <when>
          <test>
              <equal><clip pos="1" side="sl" part="a_tense"/><lit-tag v="pri"/></equal>
          </test>
          <let>
            <clip pos="1" side="tl" part="a_tense"/><lit-tag v="ind.prs"/>
          </let>
        </when>
        <!-- otherwise, same tense -->
      </choose>
    </def-macro>

  </section-def-macros>
  <section-rules>

    <rule comment="REGLA: COND">
      <pattern>
        <pattern-item n="cond"/>
      </pattern>
      <action>
        <let><clip pos="1" side="tl" part="a_sem"/><lit v=""/></let>
        <let><clip pos="1" side="tl" part="x_func"/><lit v=""/></let>
        <let><clip pos="1" side="tl" part="a_func"/><lit v=""/></let>
        <let><clip pos="1" side="tl" part="a_cond"/><lit-tag v="cond.prs"/></let>
        <out>
          <chunk name="cond" case="caseFirstWord">
            <tags>
              <tag><clip pos="1" side="sl" part="a_func"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="REGLA: COND QST">
      <pattern>
        <pattern-item n="cond"/>
        <pattern-item n="qst"/>
      </pattern>
      <action>
        <let><clip pos="1" side="tl" part="a_sem"/><lit v=""/></let>
        <let><clip pos="1" side="tl" part="x_func"/><lit v=""/></let>
        <let><clip pos="1" side="tl" part="a_func"/><lit v=""/></let>
        <let><clip pos="1" side="tl" part="a_cond"/><lit-tag v="cond.prs"/></let>
        <out>
          <chunk name="cond" case="caseFirstWord">
            <tags>
              <tag><clip pos="1" side="sl" part="a_func"/></tag>
            </tags>
            <mlu>
              <lu><clip pos="1" side="tl" part="whole"/></lu>
              <lu><clip pos="2" side="tl" part="whole"/></lu>
            </mlu>
          </chunk>
        </out>
      </action>
    </rule>


    <rule comment="REGLA: CONNEG">
      <pattern>
        <pattern-item n="conneg"/>
      </pattern>
      <action>
        <let><clip pos="1" side="tl" part="a_sem"/><lit v=""/></let>
        <let><clip pos="1" side="tl" part="x_func"/><lit v=""/></let>
        <let><clip pos="1" side="tl" part="a_func"/><lit v=""/></let>
        <choose>
          <when><test><equal><clip pos="1" side="tl" part="a_nbr"/><lit-tag v="pl"/></equal></test>
                <let><clip pos="1" side="tl" part="a_nbr"/><lit v=""/></let>
                <let><clip pos="1" side="tl" part="tags"/>
                     <concat><clip pos="1" side="tl" part="tags"/><lit-tag v="p3.pl"/></concat></let>
          </when>
        </choose>
        <choose>
          <when><test><equal><clip pos="1" side="tl" part="a_temps"/><lit-tag v="prs"/></equal></test>
                <let><clip pos="1" side="tl" part="a_temps"/><lit v=""/></let>
          </when>
        </choose>

        <out>
          <chunk name="conneg" case="caseFirstWord">
            <tags>
              <tag><clip pos="1" side="sl" part="a_func"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>



    <rule comment="REGLA: pron">
      <pattern>
        <pattern-item n="pron"/>
      </pattern>
      <action>
        <call-macro n="remove-semtag1"><with-param pos="1"/></call-macro>
        <let><clip pos="1" side="tl" part="x_func"/><lit v=""/></let>
        <let><clip pos="1" side="tl" part="a_func"/><lit v=""/></let>
        <let><clip pos="1" side="tl" part="a_poss"/><lit v=""/></let>
	<choose><when>
		<test><equal><clip pos="1" side="tl" part="a_cas"/><lit-tag v="acc"/></equal></test>
                 <let><clip pos="1" side="tl" part="a_cas"/><lit-tag v="par"/></let>
         </when></choose>

        <out>
          <chunk name="pron" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SN"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
            </lu>
          </chunk>
        </out>
      </action>
    </rule>

