            </lu>
          </chunk>
        </out>
      </action>
    </rule>

    <rule comment="REGLA: nom kaa">
      <pattern>
          <pattern-item n="nom"/>
          <pattern-item n="kanssa"/>
      </pattern>
      <action>
        <call-macro n="remove-semtag1"><with-param pos="1"/></call-macro>
        <let><clip pos="1" side="tl" part="x_func"/><lit v=""/></let>
        <let><clip pos="1" side="tl" part="a_func"/><lit v=""/></let>
        <let><clip pos="1" side="tl" part="a_poss"/><lit v=""/></let>
        <let><clip pos="1" side="tl" part="a_cas"/><lit-tag v="com"/></let>
        <out>
          <chunk name="nom" case="caseFirstWord">
            <tags>
              <tag><lit-tag v="SN"/></tag>
            </tags>
            <lu>
              <clip pos="1" side="tl" part="whole"/>
